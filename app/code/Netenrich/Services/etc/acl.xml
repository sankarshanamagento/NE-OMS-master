<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Netenrich_Services::services" title="Services" sortOrder="40">
                    <resource id="Netenrich_Services::manage" title="All Services" sortOrder="10" >
                        <resource id="Netenrich_Services::save" title="Save Services" sortOrder="10" />
                        <resource id="Netenrich_Services::services_delete" title="Delete Services" sortOrder="20" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Netenrich_Services::config_services" title="Services Section" sortOrder="50" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
